pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
                Sleep 4
            }
        }
        stage('Build') {
            steps {
                echo 'Hello Build'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Hello Deploy'
            }
        }
        stage('Test') {
            steps {
                echo 'Hello Test'
            }
        }
    }
}
